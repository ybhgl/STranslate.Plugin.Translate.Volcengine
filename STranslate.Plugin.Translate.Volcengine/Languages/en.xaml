<ResourceDictionary
    xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
    xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
    xmlns:sys="clr-namespace:System;assembly=mscorlib">

    <sys:String x:Key="STranslate_Plugin_Translate_Volcengine_Url">Url</sys:String>
    <sys:String x:Key="STranslate_Plugin_Translate_Volcengine_Url_Description">Configure the Volcengine API service address.&#x0a;If the Path is not filled in, it will be automatically populated with /api/v3/responses.&#x0a;For example, for the Volcengine API address https://ark.cn-beijing.volces.com/api/v3/responses, the Path is /api/v3/responses.</sys:String>
    <sys:String x:Key="STranslate_Plugin_Translate_Volcengine_ApiKey">ApiKey</sys:String>
    <sys:String x:Key="STranslate_Plugin_Translate_Volcengine_ApiKey_Placeholder">Please enter your apiKey</sys:String>
    <sys:String x:Key="STranslate_Plugin_Translate_Volcengine_ApiKey_Description">Enter your Volcengine API key</sys:String>
    <sys:String x:Key="STranslate_Plugin_Translate_Volcengine_Model">Select a model</sys:String>
    <sys:String x:Key="STranslate_Plugin_Translate_Volcengine_Prompt_Header">Prompt Configuration</sys:String>
    <sys:String x:Key="STranslate_Plugin_Translate_Volcengine_Prompt_Description">Select or edit the prompt template for translation</sys:String>
    <sys:String x:Key="STranslate_Plugin_Translate_Volcengine_Prompt_Placeholder">Select a prompt template</sys:String>
    <sys:String x:Key="STranslate_Plugin_Translate_Volcengine_EditPrompt_ToolTip">Edit Prompt</sys:String>
    <sys:String x:Key="STranslate_Plugin_Translate_Volcengine_EditPrompt_Content">Edit</sys:String>
    <sys:String x:Key="STranslate_Plugin_Translate_Volcengine_Temperature">Temperature Control</sys:String>
    <sys:String x:Key="STranslate_Plugin_Translate_Volcengine_Temperature_Description">Controls the randomness of the model output</sys:String>
    <sys:String x:Key="STranslate_Plugin_Translate_Volcengine_Thinking">Thinking Mode</sys:String>
    <sys:String x:Key="STranslate_Plugin_Translate_Volcengine_Thinking_Description">Controls whether the model performs deeper thinking and reasoning</sys:String>

</ResourceDictionary>